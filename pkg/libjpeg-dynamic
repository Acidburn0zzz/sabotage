[deps]

[build]
CFLAGS="-fPIC -include stdlib.h -include stdio.h" ./configure --prefix="$butch_prefix" \
  --disable-dso || exit 1

make V=1 -j$MAKE_THREADS || exit 1

# this installs .so, .a (fPIC) and .h
make DESTDIR="$butch_install_dir" install || exit 1
