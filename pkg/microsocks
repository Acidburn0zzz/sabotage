[build]
printf "LDFLAGS += -static %s\n" "$optldflags" > config.mak
printf "CFLAGS += %s\n" "$optcflags" >> config.mak

make -j$MAKETHREADS
make DESTDIR="$butch_install_dir" prefix="$butch_prefix" install


