[build]
CC="$R/bin/musl-gcc -static" ./configure --prefix=/ --disable-nls --disable-shared
echo "#define __pid_t int" >include/features.h
make CC="$R/bin/musl-gcc -static"
make DESTDIR=$R tooldir=/ install
