[build]
patch -p1 < "$K"/butch-0.0.4-fix-wget.patch || exit 1
patch -p1 < "$K"/butch-0.0.4-fix-wget_b.patch || exit 1
CC="$R/bin/musl-gcc -static" ./build.sh
cp butch/butch $R/bin/butch
